{
  "family": "springbootapp",
  "networkMode": "awsvpc",
  "executionRoleArn": "arn:aws:iam::761018887005:role/ECS-test-role",
  "containerDefinitions": [
    {
      "name": "springbootapp",
      "image": "${docker_image_url}",
      "essential": true,
      "portMappings": [
        {
          "containerPort": 8080
        }
      ],
      "environment": [
        {
          "name": "spring_profiles_active",
          "value": "default"
        }
      ],
      "logConfiguration": {
        "logDriver": "awslogs",
        "options": {
          "awslogs-group": "springbootapp-LogGroup",
          "awslogs-region": "us-east-1",
          "awslogs-stream-prefix": "springbootapp-LogGroup-stream"
        }
      }
    }
  ],
  "requiresCompatibilities": ["FARGATE"],
  "cpu": "512",
  "memory": "1024"
}
